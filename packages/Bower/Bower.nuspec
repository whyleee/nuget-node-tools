        <tags>package manager javascript front-end git</tags>
        <dependencies>
            <dependency id="Node.js" version="0.10.28" />
            <dependency id="NoGit" version="0.0.8" />
        </dependencies>
    </metadata>
    <files>
        <file src="**\*" exclude="content\_bin\bower.cmd" />
        <file src="content\_bin\bower.cmd" target="content\.bin\bower.cmd" />
    </files>
</package>